# This file specifies files that are *not* uploaded to Google Cloud
# using gcloud app deploy. It follows the same syntax as .gitignore.

# Node modules - these are created during Cloud Build and should NOT be uploaded
# App Engine will run its own install
node_modules/
**/node_modules/
.pnpm-store/

# Build caches
.turbo/
**/.turbo/
*.turbo
.next/
**/.next/

# Frontend apps (not needed for backend-only deployment)
apps/admin/
apps/website/
apps/hacker/
apps/server-e2e/

# Git
.git/

# IDE
.idea/
.vscode/
*.swp
*.swo

# Tests
coverage/
**/*.spec.ts
**/*.test.ts
jest.config.*

# Logs
*.log

# OS files
.DS_Store
Thumbs.db
