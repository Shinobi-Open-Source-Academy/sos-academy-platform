{
  "name": "admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev -p 3001",
        "cwd": "apps/admin"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next build",
        "cwd": "apps/admin"
      },
      "configurations": {
        "production": {
          "command": "NODE_ENV=production next build"
        }
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start -p 3001",
        "cwd": "apps/admin"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome lint apps/admin",
        "cwd": "."
      }
    }
  }
}
